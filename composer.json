{
  "name": "codeat3/laravel-page-speed",
  "description": "Laravel Page Speed",
  "keywords": [
    "laravel",
    "page speed",
    "minify",
    "optimize",
    "html"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Renato Marinho",
      "email": "renato.marinho@s2move.com"
    },
    {
        "name": "Swapnil Sarwe",
        "email": "swapnilsarwe@gmail.coms"
      }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0 || ^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0|^7.0",
    "squizlabs/php_codesniffer": "^3.4",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Codeat3\\LaravelPageSpeed\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codeat3\\LaravelPageSpeed\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Codeat3\\LaravelPageSpeed\\ServiceProvider"
      ]
    }
  }
}
